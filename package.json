{
  "name": "sitepreview",
  "version": "0.1.0",
  "bin": {
    "sitepreview": "bin/sitepreview.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "synth": "cdk synth \"SitePreviewStack-${STAGE}\" --require-approval never --outputs-file cdk.out.json",
    "deploy": "cdk deploy \"SitePreviewStack-${STAGE}\" --require-approval never --outputs-file cdk.out.json",
    "postdeploy": "node --eval \"console.log('::set-output name=env_url::' + require('./cdk.out.json')['SitePreviewStack-${STAGE}'].DeploymentUrl)\"",
    "destroy": "cdk destroy \"SitePreviewStack-${STAGE}\" --force"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/jest": "^29.2.5",
    "@types/node": "18.11.18",
    "aws-cdk": "^2.61.1",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4",
    "@aws-cdk/aws-cloudfront": "^1.189.0",
    "@aws-cdk/aws-cloudfront-origins": "^1.189.0",
    "@aws-cdk/aws-s3": "^1.189.0",
    "@aws-cdk/aws-s3-deployment": "^1.189.0",
    "@aws-cdk/core": "^1.189.0",
    "aws-cdk-lib": "2.61.1",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
